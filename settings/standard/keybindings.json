// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "cmd+left",
        "command": "cursorLineStart",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+a",
        "command": "-cursorLineStart",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+right",
        "command": "cursorLineEnd",
        "when": "textInputFocus"
    },
    {
        "key": "ctrl+e",
        "command": "-cursorLineEnd",
        "when": "textInputFocus"
    },
    {
        "key": "f5",
        "command": "-vscode-postgres.runQuery",
        "when": "editorLangId == 'postgres'"
    },
    {
        "key": "cmd+enter",
        "command": "vscode-postgres.runQuery"
    }
]